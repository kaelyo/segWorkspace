package com.gboss.pojo;

public class FankongCmdInfo {
	private String simIds;
	private String opType;
	
	private String remoteCtrlStatus;
	public String getSimIds() {
		return simIds;
	}
	public void setSimIds(String simIds) {
		this.simIds = simIds;
	}
	public String getOpType() {
		return opType;
	}
	public void setOpType(String opType) {
		this.opType = opType;
	}
	public String getRemoteCtrlStatus() {
		return remoteCtrlStatus;
	}
	public void setRemoteCtrlStatus(String remoteCtrlStatus) {
		this.remoteCtrlStatus = remoteCtrlStatus;
	}
	
	
}
