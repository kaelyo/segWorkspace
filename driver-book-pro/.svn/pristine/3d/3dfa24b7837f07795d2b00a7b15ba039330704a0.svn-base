package com.chinagps.driverbook.pojo;


/**
 * 联系人POJO
 * 
 * @author Ben
 *
 */
public class Linkman extends BaseEntity {
	private static final long serialVersionUID = -9098047351905908280L;

	/** 联系人ID */
	private Long linkmanId;
	/** 客户ID */
	private Long customerId;
	/** 车辆ID, 集客不一定对应车辆, 可填0 */
	private Long vehicleId;
	/** 类型 1：第一联系人/常用联系人, 2：第二联系人/报警联系人, 99：其它 */
	private Integer linkmanType;
	/** 联系人姓名 */
	private String linkman;
	/** 电话号码 */
	private String phone;

	public Long getLinkmanId() {
		return linkmanId;
	}

	public void setLinkmanId(Long linkmanId) {
		this.linkmanId = linkmanId;
	}

	public Long getCustomerId() {
		return customerId;
	}

	public void setCustomerId(Long customerId) {
		this.customerId = customerId;
	}

	public Long getVehicleId() {
		return vehicleId;
	}

	public void setVehicleId(Long vehicleId) {
		this.vehicleId = vehicleId;
	}

	public Integer getLinkmanType() {
		return linkmanType;
	}

	public void setLinkmanType(Integer linkmanType) {
		this.linkmanType = linkmanType;
	}

	public String getLinkman() {
		return linkman;
	}

	public void setLinkman(String linkman) {
		this.linkman = linkman;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}

}
