<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">

<struts>
	<package name="sys" namespace="/sys" extends="global">
		<!-- 终端供应商字典-->
		<action name="dicSupperier_*" class="dicSupperiertAction" method="{1}">
		</action>
		
		<!-- 终端类型字典-->
		<action name="dicUnitType_*" class="dicUnitTypeAction" method="{1}">
		</action>
		
		<!-- 操作日志字典-->
		<action name="operatelog_*" class="operateLogAction" method="{1}">
		</action>
		
		<!-- 系统参数设置-->
		<action name="sysparam_*" class="systemParamAction" method="{1}">
		</action>
		
		<!-- 行政区域-->
		<action name="maparea_*" class="mapChinaAreaAction" method="{1}">
		</action>
	</package>
</struts>