package cc.chinagps.gateway.unit.pengaoda.upload.beans;

public class PengAoDaFaultCode {
	private String faultCode;
	
	//01表示确定故障码 02表示未决故障码
	private int faultCodeType;

	public String getFaultCode() {
		return faultCode;
	}

	public void setFaultCode(String faultCode) {
		this.faultCode = faultCode;
	}

	public int getFaultCodeType() {
		return faultCodeType;
	}

	public void setFaultCodeType(int faultCodeType) {
		this.faultCodeType = faultCodeType;
	}
	
	@Override
	public String toString() {
		StringBuilder sb = new StringBuilder();
		sb.append("{faultCode:").append(faultCode);
		sb.append(", faultCodeType:").append(faultCodeType);
		sb.append("}");
		
		return sb.toString();
	}
}