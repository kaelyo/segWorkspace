<script type="text/javascript">
 // 兼容代码
 if (!console) {
  window.console = {
   log: function(msg){},
   error: function(msg){}
  };
 }
 function compatibleArray(array){
   if (!array) {array = [];}
   array.value = {
    split : function() {
     return array;
    }
   };
   return array;
 }

 var left_width = Util.getCookie("left_width");
 var map_height = Util.getCookie("map_height");
 if (left_width) {
  $("#left_panel").width(left_width);
  $(".right-panel").css("margin-left", parseInt(left_width) + 5 + "px");
 }
 if (map_height) {
  $("#map").height(map_height);
 }
 // 调整高度
 window.onresize = function() {
  var height = $(window).height();
  // 设置最大高度
  $("#_searchResultPanel").height(height - 207);
  $("#_watchListPanel").height(height - 207);
  $("#_orderMsgPanel").height(height - 207);
  $("#_position_search_result_table").height(height - 161);
  var _height = height - $("#map").height() - $("#_GPSMsgPanel").height() - 97;
  if (_height>0){$("#map").height($("#map").height()+_height);} else {
   var gps_height = $("#_GPSMsgPanel").height()+_height;
   if (gps_height<145){
    $("#map").height($("#map").height()+_height+$("#_GPSMsgPanel").height()-145);
    $("#_GPSMsgPanel").height(145);
   } else {
    $("#_GPSMsgPanel").height(gps_height);
   }
  }
 };
 window.onresize();

$(window).unload(function(){
 Util.setCookie("left_width",  $("#left_panel").width(), new Date(new Date().getTime() + 31536000000));
 Util.setCookie("map_height",  $("#map").height(), new Date(new Date().getTime() + 31536000000));
 watchListCookie = "";
 $("#watchListPanel table>tbody>tr").each(function(index,ele){
  watchListCookie += (ele.getAttribute("call_letter") + "||" + ele.getAttribute("plate_no")) + "||";
 });
 Util.setCookie("watch_list", watchListCookie, new Date(new Date().getTime() + 31536000000));
});
</script>
