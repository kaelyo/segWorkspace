 <!-- 实时跟踪模态框 -->
 <div class="modal fade" id="trailModal" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog" style="max-width:350px;">
   <div class="modal-content">
    <div class="modal-header">
     <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
     <h5 class="modal-title">跟踪</h5>
    </div>
    <div class="modal-body" style="padding-bottom: 0;">
     <div class="panel panel-default nogap">
      <div class="panel-body" style="padding-top: 0;">
       <h6>选择的车辆：</h6>
       <div id="trail-cars" style="width: 80%; margin-left: 10px;"></div>
       <div class="input-group input-group-sm" style="margin-top: 8px;">
        <span class="input-group-addon noborder">跟踪次数：</span><input id="trail-times" class="form-control" value="3"><span
         class="input-group-addon noborder nobgcolor"></span><span class="input-group-addon noborder">时间间隔(秒)：</span><input id="trail-interval"
         class="form-control" value="10">
       </div>
      </div>
     </div>
    </div>
    <div class="modal-footer noborder">
     <button id="trail-start" type="button" class="btn btn-primary" data-dismiss="modal">开始跟踪</button>
     <button id="trail-stop" type="button" class="btn btn-primary" data-dismiss="modal">停止跟踪</button>
     <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
    </div>
   </div>
  </div>
 </div>
 <!-- 历史查询模态框  -->
 <div class="modal fade" id="historyModal" aria-hidden="true">
  <div class="modal-dialog" style="max-width:350px;">
   <div class="modal-content">
    <div class="modal-header">
     <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
     <h5 class="modal-title">
      历史回放 [ <span id="history-car"></span> ]
     </h5>
    </div>
    <div class="modal-body" style="padding-bottom: 0;">
     <form class="form-horizontal" role="form">
      <div class="form-group">
       <label for="play_history_start_time" class="col-sm-4 control-label">开始时间</label>
       <div class="col-sm-7">
        <input type="text" class="form-control form_datetime" id="play_history_start_time" placeholder="请输入开始时间">
       </div>
      </div>
      <div class="form-group">
       <label for="play_history_end_time" class="col-sm-4 control-label">结束时间</label>
       <div class="col-sm-7">
        <input type="text" class="form-control form_datetime" id="play_history_end_time" placeholder="请输入结束时间">
       </div>
      </div>
      <div class="row text-center" style="margin-top: 15px">
       <div id="play_history_dlg_search_result" style="display: block; color: rgb(71, 164, 71);"></div>
      </div>
     </form>
    </div>
    <div class="modal-footer noborder" style="padding-right: 50px;">
     <button id="history-start" type="button" class="btn btn-primary">查询</button>
     <button id="play_history_dlg_play_btn" type="button" data-dismiss="modal" class="btn btn-primary disabled">播放</button>
     <button id="play_history_dlg_list_btn" type="button" data-dismiss="modal" class="btn btn-primary disabled">列点</button>
    </div>
   </div>
  </div>
 </div>
 <!-- 车台复位模态框 -->
 <div class="modal fade" id="rebackModal" aria-hidden="true">
  <div class="modal-dialog" style="max-width:280px;">
   <div class="modal-content">
    <div class="modal-header">
     <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
     <h5 class="modal-title">车台复位</h5>
    </div>
    <div class="modal-body">
     您确定要对以下车台下发复位指令吗？
     <div id="reback-cars" style="width: 80%; resize: none; margin: 10px;"></div>
    </div>
    <div class="modal-footer noborder" style="padding-right: 50px;">
     <button id="history-start" type="button" class="btn btn-primary" data-dismiss="modal"
      onclick="Reback(getSelectRow('#carList-tabPanel>.active table','call_letter'),getSelectRow('#carList-tabPanel>.active table','plate_no'));">是</button>
     <button type="button" class="btn btn-default" data-dismiss="modal">否</button>
    </div>
   </div>
  </div>
 </div>
 <!-- 地图模态框 -->
 <div class="modal fade" id="add_static_marker_config_dlg" aria-hidden="true">
  <div class="modal-dialog">
   <div class="modal-content">
    <div class="modal-header">
     <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
     <h5 class="modal-title">增加标注</h5>
    </div>
    <div class="modal-body" style="padding-bottom: 0;">
     <form class="form-horizontal" role="form">
      <div class="form-group">
       <label for="play_history_start_time" class="col-sm-3 control-label">名称:</label>
       <div class="col-sm-7">
        <input id="add_static_marker_name" type="text" class="form-control" placeholder="标注名称">
       </div>
      </div>
      <div class="form-group">
       <label for="play_history_start_time" class="col-sm-3 control-label">经度:</label>
       <div class="col-sm-7">
        <input id="add_static_marker_lon" type="text" class="form-control" placeholder="标注经度">
       </div>
      </div>
      <div class="form-group">
       <label for="play_history_start_time" class="col-sm-3 control-label">纬度:</label>
       <div class="col-sm-7">
        <input id="add_static_marker_lat" type="text" class="form-control" placeholder="标注纬度">
       </div>
      </div>
     </form>
     <div class="modal-footer noborder">
      <button type="button" class="btn btn-default" id="add_static_marker_ok_btn">确定</button>
      <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
     </div>
    </div>
   </div>
  </div>
 </div>
