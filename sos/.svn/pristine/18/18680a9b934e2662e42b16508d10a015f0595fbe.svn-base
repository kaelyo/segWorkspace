</body>
<script type="text/javascript">
(function($){
	//初始化表格
	 	var defaults = {
		        width:  650,
		        height: 100,
		        url:'',
	       	    rownumbers:true,//序号
	            isCount:true,
		        colModel : [
		            {display: '联系人', name : 'person', width : 100, sortable : false},
		            {display: '联系人电话', name : 'phone', width : 120, sortable : false},
		            {display: '预约地点', name : 'place', width : 120, sortable : false},
		            {display: '预约时间', name : 'time', width : 100, sortable : false},
		            {display: '任务类型', name : 'type', width : 100, sortable : false}
		        ]
		    };
		    $('#reservation_history').sgDatagrid(defaults);
		    
})(jQuery)

