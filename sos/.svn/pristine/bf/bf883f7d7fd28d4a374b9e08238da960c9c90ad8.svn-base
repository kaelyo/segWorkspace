    var save = function() {
    	$("#repair_content").on('submit',
				function(e) {
			$.ajax({
				type : 'post',
				async : false,
				contentType : 'application/json',
				dataType : 'json',
				url : '../../repair/add',
				data : JSON.stringify({
					customer_id : $('#cust_id').val(),
					vehicle_id : $('#vehicle_id').val(),
					unit_id : $('#unit_id').val(),
					repair_no : $('#repair_no', '#repair_content').val(),
					symptom : $('#symptom', '#repair_content').val(),
					exclude : $('#exclude', '#repair_content').val(),
					worker : $('#worker', '#repair_content').val(),
					place : $('#place', '#repair_content').val(),
					receptionist : $('#receptionist', '#repair_content').val(),
					amount : $('#amount', '#repair_content').val(),
					upgrade_fee : $('#upgrade_fee', '#repair_content').val(),
					ticket_no : $('#ticket_no', '#repair_content').val(),
					repair_date : $('#repair_date', '#repair_content').val(),
					remark : $('#remark', '#repair_content').val()
				}),
				success : function(data) {
					if (data) {
						if (data.success) {
							$(document).sgWindow('close', {
								id : 'repair_window'
							});
							$('#repairGrid').sgDatagrid('reload','sgDatagrid');
						}
						$(document).sgPup({message:'message_alert',text: data.msg});
					}
				},
				error : function(res, error) {
					alert("responseText=" + res.responseText
							+ ";error=" + error);
				}
			});
			$('#repair_content').unbind();//以下两行可以阻止提交2次
			e.stopPropagation();
			return false;
		});
    }
    
