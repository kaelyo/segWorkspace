	$("#sysHm_button").on("click", function(){
		var customerId = $("#cust_id").val();
		if(sysHmFlag){
			$(document).sgPup({message:'message_alert',text: '正在进行同步，请稍候...'});
			return;
		}
		var vehicleId = $("#vehicle_id").val();
		if(customerId == "" || vehicleId == ""){
			return;
		}
		sysHmFlag = true;
		$.ajax({
			type : 'post',
			async: false,   
			contentType : 'application/json',     
			dataType : 'json',     
			url : '../../changeServicePwdT2Hm',   
			data:JSON.stringify({customerId:customerId,vehicleId:vehicleId}),
			success : function(data) {
				if(data){
					sysHmFlag = false;
					$(document).sgPup({message:'message_alert',text: data.msg});
				}
			} ,   
			error : function(res,error) {
