saveServicePwd = function(){
	$(document).sgConfirm({text: '确定修改服务密码吗?',cfn:function(r){
		if(r){
			var customerId = $("#cust_id").val();
			var vehicleId = $("#vehicle_id").val();
			var unitId = $("#unit_id").val();
			var custType = $("#custType").val();
			var servicePwd = $("#service_pwd", "#sys_pwd_form").val();
			var cfservicePwd = $("#service_pwd_cf", "#sys_pwd_form").val();
			if(servicePwd != cfservicePwd){
				$(document).sgPup({message:'message_alert',text: "密码不一致，请确认密码!"});
				return false;
			}
			var updateType = "1";
			var params = {customerId:customerId, vehicleId:vehicleId, unitId:unitId, custType:custType, servicePwd:servicePwd, updateType:updateType,};
			if(vehicleId == ""){
				$(document).sgPup({message:'message_alert',text: "请先查询再修改!"});
				return false;
			}
			$.ajax({
				type : 'post',
				async : false,
				contentType : 'application/json',
				dataType : 'json',
				url : '../../updateNetworkPwd',
				data : JSON.stringify(params),
				success : function(data) {
					if (data) {
						alert(data.msg);
						if(data.success){
							$(document).sgWindow('close', {id : 'service_pwd_window'});
						}
					}
				},
				error : function(res, error) {
					$(document).sgPup({message:'message_alert',text: "responseText=" + res.responseText
						+ ";error=" + error});
				}
			});
		}
	}});
}

