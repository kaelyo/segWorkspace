/*
定义右键下拉式菜单的弹出框
*/
.contextmentmodal {
	tabindex="-1";
	position:absolute;
	z-index:9999;
	display:none;	
}

.contextmentmodal ul {
	display:block;
	position:static;	
}

/*
定义tab标签页的关闭图标
*/
.gboss-tabs {
  border-bottom: 1px solid #AAAAAA;
}

.gboss-tabs .gboss-tabs-close {
	position: relative;
	display: inline-block;
	font-family: 'Glyphicons Halflings';
	font-style: normal;
	font-weight: normal;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
	float: left;
	cursor:pointer;
	padding:8px 10px 6px 0px;
}

.gboss-tabs .gboss-tabs-close:before {
  content: "\e014";
}

.gboss-tabs > li {
  float: left;
  border: 1px solid #AAAAAA;
  border-radius: 15px 15px 0 0;
  background-image: url(../images/bg-blue.gif);
  background-size: 100% 100%;
  margin-bottom: -1px;
}
.gboss-tabs > li > a {
  line-height: 1.428571429;
  float: left;
  padding: 3px 6px;
}

.gboss-tabs > li > a:hover {
  border-radius: 15px 0px 0 0;
}

.gboss-tabs > li.active > a,
.gboss-tabs > li.active > a:hover,
.gboss-tabs > li.active > a:focus {
	color: #555555;
	background-color: transparent;
	cursor: default;
}

/* 取消 nav li 的 hover focus 底色*/
.gboss-tabs > li > a:hover,
.gboss-tabs > li > a:focus {
	color: #555555;
	background-color: transparent;
}

.gboss-tabs > li.active,
.gboss-tabs > li.active:hover,
.gboss-tabs > li.active:focus {
  background-image: none;
  background-color: #FFFFFF;
  border: 1px solid #AAAAAA;
  border-radius: 15px 15px 0 0;
  border-bottom-color: transparent;
}

