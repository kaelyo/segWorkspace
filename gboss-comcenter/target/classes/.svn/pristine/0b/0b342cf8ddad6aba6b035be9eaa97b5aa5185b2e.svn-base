	//
	private void btnTestActionPerformed(java.awt.event.ActionEvent evt) {
		bstarttest = !bstarttest;
		if (bstarttest) {
			long callletter = Long.valueOf(this.textCaller.getText());
			int calllettercount = Integer.valueOf(this.textCount.getText());
			int gpsinterval = Integer.valueOf(this.textInterval.getText()) * 1000;
			wgps = new WriteActiveMQGPS(callletter, calllettercount, gpsinterval);
			writestarttime = System.currentTimeMillis();
			wgps.start();
			this.btnTest.setLabel("Stop GPS");
		} else {
			wgps.Stop();
			this.btnTest.setLabel("Start GPS");
			wgps = null;
		}
	}

	private void btnSeatActionPerformed(java.awt.event.ActionEvent evt) {
		bstartseat = !bstartseat;
		if (bstartseat) {
			long callletter = Long.valueOf(this.textCaller.getText());
			int calllettercount = Integer.valueOf(this.textCount.getText());
			int seatcount = Integer.valueOf(this.textSeat.getText());
			try {
				clientmanager = new WebSocketClientManager(textComCenter.getText(), seatcount);
				clientmanager.run(callletter, calllettercount);
			} catch (Exception e) {
				e.printStackTrace();
				if (clientmanager != null) {
					clientmanager.Stop();
					clientmanager = null;
				}
			}
			this.btnSeat.setLabel("Stop Seat");
		} else {
			this.btnSeat.setLabel("Start Seat");
			clientmanager.Stop();
			clientmanager = null;
		}
	}

