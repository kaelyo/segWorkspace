	private static final long serialVersionUID = 1L;
	public static long writestarttime = 0;
	public static long writestoptime = 0;
	public static AtomicLong writegpscount = new AtomicLong(0);
	public static AtomicLong delivercount = new AtomicLong(0);
	public static AtomicLong lastoktimes = new AtomicLong(0);
	public static AtomicLong lasterrortimes = new AtomicLong(0);
	public static AtomicLong historyoktimes = new AtomicLong(0);
	public static AtomicLong historyerrortimes = new AtomicLong(0);

	@Override
	//定时刷新状态
	public void run() {
		while (true) {
			try {
				Thread.sleep(1000);
				if (bstarttest) {
					writestoptime = System.currentTimeMillis();
				}
				long writetime = writestoptime - writestarttime;
				if (writetime <= 0)
					writetime = 1;
				String strStatus = String.format("GPS:%d(%d), Deliver:%d, Last:%d(%d), History:%d(%d)", writegpscount.get(), writegpscount.get() * 1000 / writetime, delivercount.get(), lastoktimes.get(), lasterrortimes.get(),
						historyoktimes.get(), historyerrortimes.get());
				this.textStatus.setText(strStatus);
			} catch (Exception e) {
				e.printStackTrace();
			}
		}
	}

