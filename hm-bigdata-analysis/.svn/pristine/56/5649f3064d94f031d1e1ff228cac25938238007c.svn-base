.main-menu a {
	font-size: 14px;
	display: block;
	padding-left: 20px;
	cursor: pointer;
	text-decoration: none;
}

.main-menu .icon {
	margin-right: 10px;
	margin-top: -4px;
}

.main-menu .main-item {
	-webkit-transition: padding 0.25s linear;
	-ms-transition: padding 0.25s linear;
	transition: padding 0.25s linear;
}

.main-menu .main-item.active, .main-menu .main-item.selected,.main-menu .main-item.wnf_active {
	/* border-left: 2px solid #fff; */
	box-shadow: inset 2px 0 1px 0px rgba(0, 0, 0, 0.2), inset 0 0 4px 2px
		rgba(0, 0, 0, 0.1);
	border-top: none;
	border-bottom: 1px solid #59a0dc;
	background: #3076b3;
}

.main-menu .main-item.active .sub-menu, .main-menu .main-item.selected .sub-menu,.main-menu .main-item.wnf_active .sub-menu
	{
	height: auto;
}

.main-menu .main-item.active,.main-menu .main-item.wnf_active {
	padding-bottom: 10px;
}

.main-menu .main-item:hover:not (.active ) {
	background: #59a0dc;
}

.main-menu .main-link {
	line-height: 60px;
}

.sub-menu {
	position: relative;
	top: -10px;
	height: 0;
	overflow: hidden;
	-webkit-transition: all 0.25s linear;
	-ms-transition: all 0.25s linear;
	transition: all 0.25s linear;
	margin-left: 14px;
	font-size: 12px;
}

.sub-menu .sub-menu {
	margin-top: 10px;
}

.sub-menu:before {
	content: '';
	position: absolute;
	left: 20px;
	top: 20px;
	bottom: 13px;
	width: 0;
	border-left: 1px solid #7fc2fc;
}

.sub-menu li:nth-child(1)>a:before {
	border-left: none;
}

.sub-menu li:hover {
	text-shadow: 0 10px 10px #2b77ba;
}

.sub-menu li.selected a {
	color: #ebf0f6;
}

.sub-menu li.selected a span {
	background-color: #1eb0da;
	border-radius: 6px;
	padding: 0 6px;
}

.sub-menu li.selected>a:after {
	border: 1px solid #fff;
	width: 9px;
	height: 9px;
	background: none;
	top: 15px;
	background: #fff;
	box-shadow: inset 0 0 0 2px #3076b3;
}

.sub-menu li a {
	font-size: 12px;
	padding: 12px 0 4px 50px;
	color: #abcced;
	position: relative;
}

.sub-menu li a:before {
	content: '';
	position: absolute;
	height: 100%;
	width: 20px;
	border-bottom: 1px solid #7fc2fc;
	top: -13px;
	left: 20px;
}

.sub-menu li a:after {
	content: '';
	position: absolute;
	width: 9px;
	height: 9px;
	background: #7fc2fc;
	border-radius: 50%;
	left: 38px;
	top: 16px;
}

