.site-header {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	background: #4295dd;
	height: 80px;
	border-bottom: 1px solid #3071ab;
	font-size: 16px;
}

.site-header, .site-header a {
	color: #fff;
	text-decoration: none;
}

.site-header .logo {
	position: absolute;
	width: 230px;
	height: 80px;
	background: url(../../images/logo.png) no-repeat 0 center;
	border-right: 1px solid #3071ab;
	cursor: pointer;
}
@font-face {
  font-family: 'headericonfont';
  src: url('../fonts/iconfont.eot');
  src: url('../fonts/iconfont.eot?#iefix') format('embedded-opentype'),
  url('../fonts/iconfont.woff') format('woff'),
  url('../fonts/iconfont.ttf') format('truetype'),
  url('../fonts/iconfont.svg#iconfont') format('svg');
}
.naviconfont{
  font-family:"headericonfont" !important;
  font-size:20px;font-style:normal;
  -webkit-font-smoothing: antialiased;
  color: #a3e8a9;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.site-header .infos {
	position: absolute;
	right: 30px;
	margin-top: 18px;
	white-space: nowrap;
	margin-left: 32px;
	color: #e8f1fb;
}

.site-header .infos li {
	margin-right: 20px;
	display: inline-block;
}

.site-header .infos .icon {
	margin-right: 8px;
}

 .hide-tree .site-main .main-menu{
	width: 0px !important;
	overflow-x: hidden;
}

 .hide-tree .contents{
    margin-left: 1px !important;
}
.hide-tree .menu_parent{
	display:none;
}
