/*第一圈圆*/
@keyframes newsBtnFir{
	0%{opacity: 0;}
	25%{opacity: 0;}
	30%{opacity: 0.95;}
	70%{opacity: 0;}
}
@-webkit-keyframes newsBtnFir{
	0%{opacity: 0;}
	25%{opacity: 0;}
	30%{opacity: 0.95;}
	70%{opacity: 0;}
}
@-moz-keyframes newsBtnFir{
	0%{opacity: 0;}
	25%{opacity: 0;}
	30%{opacity: 0.95;}
	70%{opacity: 0;}
}
@-o-keyframes newsBtnFir{
	0%{opacity: 0;}
	25%{opacity: 0;}
	30%{opacity: 0.95;}
	70%{opacity: 0;}
}
/*第二圈圆*/
@keyframes newsBtnTwo{
	0%{opacity: 0;}
	35%{opacity: 0;}
	40%{opacity: 0.85;}
	80%{opacity: 0;}
}
@-webkit-keyframes newsBtnTwo{
	0%{opacity: 0;}
	35%{opacity: 0;}
	40%{opacity: 0.85;}
	80%{opacity: 0;}
}
@-moz-keyframes newsBtnTwo{
	0%{opacity: 0;}
	35%{opacity: 0;}
	40%{opacity: 0.85;}
	80%{opacity: 0;}
}
@-o-keyframes newsBtnTwo{
	0%{opacity: 0;}
	35%{opacity: 0;}
	40%{opacity: 0.85;}
	80%{opacity: 0;}
}
/*第三圈圆*/
@keyframes newsBtnThr{
	0%{opacity: 0;}
	40%{opacity: 0;}
	47%{opacity: 0.7;}
	90%{opacity: 0;}
}
@-webkit-keyframes newsBtnThr{
	0%{opacity: 0;}
	40%{opacity: 0;}
	47%{opacity: 0.7;}
	90%{opacity: 0;}
}
@-moz-keyframes newsBtnThr{
	0%{opacity: 0;}
	40%{opacity: 0;}
	47%{opacity: 0.7;}
	90%{opacity: 0;}
}
@-o-keyframes newsBtnThr{
	0%{opacity: 0;}
	40%{opacity: 0;}
	47%{opacity: 0.7;}
	90%{opacity: 0;}
}
.jglbUl{
	/*overflow: auto;*/
	height: 500px;
	overflow-x:auto;
	overflow-y:auto;
	width:200px;
}
.jglbUl::-webkit-scrollbar{width:4px;height: 4px;}
.jglbUl::-webkit-scrollbar-track{
	background-color:#4295dd;
	-webkit-border-radius: 2px;
    border-radius: 2px;
}
.jglbUl::-webkit-scrollbar-thumb{background-color:#2970ad;}
.jglbUl::-webkit-scrollbar-thumb:hover {background-color:#38be9e;}
.jglbUl::-webkit-scrollbar-thumb:active {background-color:#2970ad;}

