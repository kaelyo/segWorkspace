.newsBtn{width:25px;height: 25px;position: absolute;top:20px;right: 20px;background:rgba(35,117,180,1);border-radius: 50%; }
.newsBtn span{width: 25px;height: 25px;position: absolute;border-radius: 50%;background: rgba(255,255,255,0);
	/*background: rgba(35,117,180,0.3);*/opacity: 0;
	/*transform:scale(0);
	-webkit-transform:scale(0);
	-moz-transform:scale(1);
	-o-transform:scale(1);*/
}
.newsBtn span:nth-of-type(1){
	border: 1px solid #458fc9;
	transform:scale(1.3);
	-webkit-transform:scale(1.3);
	-moz-transform:scale(1.3);
	-o-transform:scale(1.3);
	/*animation:newsBtn 1.5s infinite alternate;
	-webkit-animation:newsBtn 1.5s infinite alternate;
	-o-animation:newsBtn 1.5s infinite alternate;
	-moz-animation:newsBtn 1.5s infinite alternate;*/
	animation:newsBtnFir 4s ease-in 1s infinite;
	-webkit-animation:newsBtnFir 4s ease-in 1s infinite;
	-o-animation:newsBtnFir 4s ease-in 1s infinite;
	-moz-animation:newsBtnFir 4s  ease-in 1s infinite;
}
.newsBtn span:nth-of-type(2){
	border: 1px solid #64a4d6;
	transform:scale(1.7);
	-webkit-transform:scale(1.7);
	-moz-transform:scale(1.7);
	-o-transform:scale(1.7);
	/*animation:newsBtn 1.5s 0.7s infinite alternate;
	-moz-animation:newsBtn 1.5s 0.7s infinite alternate;
	-webkit-animation:newsBtn 1.5s 0.7s infinite alternate;
	-o-animation:newsBtn 1.5s 0.7s infinite alternate;*/
	animation:newsBtnTwo 4s ease-in 1s infinite;
	-webkit-animation:newsBtnTwo 4s ease-in 1s infinite;
	-o-animation:newsBtnTwo 4s ease-in 1s infinite;
	-moz-animation:newsBtnTwo 4s ease-in 1s infinite;
}
.newsBtn span:nth-of-type(3){
	border: 1px solid #96bddb;
	transform:scale(2.2);
	-webkit-transform:scale(2.2);
	-moz-transform:scale(2.2);
	-o-transform:scale(2.2);
	/*animation:newsBtn 1.5s 1.5s infinite alternate;
	-webkit-animation:newsBtn 1.5s 1.5s infinite alternate;
	-moz-animation:newsBtn 1.5s 1.5s infinite alternate;
	-o-animation:newsBtn 1.5s 1.5s infinite alternate;*/
	animation:newsBtnThr 4s ease-in 1s infinite;
	-webkit-animation:newsBtnThr 4s ease-in 1s infinite;
	-o-animation:newsBtnThr 4s ease-in 1s infinite;
	-moz-animation:newsBtnThr 4s ease-in 1s infinite;
}
